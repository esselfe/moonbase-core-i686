kreqs="fglrx \
       fuse \
       iptables \
       NVIDIA \
       openafs-driver \
       submount \
       virtualbox-module \
       easy-slow-down-manager \
       broadcom-wl \
       spl \
       zfs"


for req in $kreqs; do
  if module_installed $req; then
    # ignore failures as much as possible
    lin -c $req || true
  fi
done
