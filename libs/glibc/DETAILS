          MODULE=glibc
         VERSION=2.32
          SOURCE=$MODULE-$VERSION.tar.xz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_URL=$GNU_URL/$MODULE
      SOURCE_VFY=sha256:1627ea54f5a1a8467032563393e0901077626dc66f37f10ee6363bb722222836
