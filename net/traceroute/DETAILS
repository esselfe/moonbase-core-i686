          MODULE=traceroute
         VERSION=2.0.20
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE
      SOURCE_VFY=sha1:4c792c76ccdbbd3265bb866675f57ab09121efee
        WEB_SITE=ftp://ftp.ee.lbl.gov
