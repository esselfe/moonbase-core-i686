          MODULE=traceroute
         VERSION=2.0.19
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE
      SOURCE_VFY=sha1:44c109cfe4950f1617d0d796b432356b9acf4bd4
        WEB_SITE=ftp://ftp.ee.lbl.gov
